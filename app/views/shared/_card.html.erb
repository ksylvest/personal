<%= tag.meta name: "twitter:card", content: "summary" %>
<%= tag.meta name: "twitter:site", content: "@ksylvest" %>
<%= tag.meta name: "twitter:title", content: yield(:title) %>
<%= tag.meta name: "twitter:description", content: yield(:description) %>
<%= tag.meta name: "twitter:image", content: image_path("profile/default.jpeg") %>
<%= tag.meta name: "twitter:image:width", content: 1600 %>
<%= tag.meta name: "twitter:image:height", content: 1600 %>
