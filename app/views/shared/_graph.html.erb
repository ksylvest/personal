<%= tag.meta property: "og:type", content: "website" %>
<%= tag.meta property: "og:title", content: yield(:title) %>
<%= tag.meta property: "og:description", content: yield(:description) %>
<%= tag.meta property: "og:image", content: image_path("profile/default.jpeg") %>
<%= tag.meta property: "og:url", content: request.url %>
